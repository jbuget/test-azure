{
  "name": "test-azure",
  "version": "1.0.0",
  "description": "REST API to test Microsoft Azure platform (with PostgreSQL) ",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --write .",
    "prisma:generate": "node node_modules/prisma/build/index.js generate",
    "prisma:migrate": "node node_modules/prisma/build/index.js migrate dev",
    "prisma:deploy": "node node_modules/prisma/build/index.js migrate deploy",
    "prisma:studio": "node node_modules/prisma/build/index.js studio",
    "postinstall": "npm run prisma:generate",
    "prestart": "npm run prisma:deploy && npm run prisma:generate",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbuget/test-azure.git"
  },
  "author": "Jérémy Buget <contact@jbuget.fr>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbuget/test-azure/issues"
  },
  "homepage": "https://github.com/jbuget/test-azure#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/view": "^11.1.1",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "fastify": "^5.6.1",
    "fastify-healthcheck": "^5.1.0",
    "prisma": "^6.16.2"
  }
}
